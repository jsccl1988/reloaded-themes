{
  "name": "Dark-C++",
  "settings": [
    { "scope": ["emphasis"]                            , "settings": { "fontStyle": "italic"    } },
    { "scope": ["strong"]                              , "settings": { "fontStyle": "bold"      } },

    { "scope": ["markup.underline"]                    , "settings": { "fontStyle": "underline" } },
    { "scope": ["markup.bold"]                         , "settings": { "fontStyle": "bold"      } },
    { "scope": ["markup.heading"]                      , "settings": { "foreground": "#6796e6"  } },
    { "scope": ["markup.italic"]                       , "settings": { "fontStyle": "italic"    } },
    { "scope": ["markup.inserted"]                     , "settings": { "foreground": "#b5cea8"  } },
    { "scope": ["markup.deleted"]                      , "settings": { "foreground": "#ce9178"  } },
    { "scope": ["markup.changed"]                      , "settings": { "foreground": "#569cd6"  } },

    { "scope": ["header"]                              , "settings": { "foreground": "#000080"  } },
    { "scope": ["invalid"]                             , "settings": { "foreground": "#f44747"  } },

    { "scope": ["comment"]                             , "settings": { "foreground": "#606060"  } },
    { "scope": ["comment.line.banner"]                 , "settings": { "fontStyle" : "bold"     } },
    { "scope": ["comment.code",
                "comment.code-punctuation"]            , "settings": { "foreground": "#afafaf"  } },
    { "scope": ["comment.command"]                     , "settings": { "foreground": "#386ea7"  } },
    { "scope": ["comment.variable"]                    , "settings": { "foreground": "#386ea7"  } },
    { "scope": ["comment.section"]                     , "settings": { "foreground": "#7d7d7d", "fontStyle": "bold" }  },
    { "scope": ["comment.decoration"]                  , "settings": { "foreground": "#7d7d7d"  } },

    { "scope": ["keyword"]                             , "settings": { "foreground": "#569cd6"  } },

    { "scope": ["constant.boolean"]                    , "settings": { "foreground": "#569cd6"  } },
    { "scope": ["constant.numeric"]                    , "settings": { "foreground": "#78cf8a"  } },
    { "scope": ["constant.number-suffix"]              , "settings": { "foreground": "#478a55"  } },
    { "scope": ["constant.regexp"]                     , "settings": { "foreground": "#b46695"  } },
    { "scope": ["constant.rgb-value"]                  , "settings": { "foreground": "#d4d4d4"  } },

    { "scope": ["entity.name.tag"]                     , "settings": { "foreground": "#569cd6"  } },
    { "scope": ["entity.name.selector"]                , "settings": { "foreground": "#d7ba7d"  } },
    { "scope": ["entity.other.attribute-name"]         , "settings": { "foreground": "#9cdcfe"  } },
    { "scope": ["entity.other.attribute-name.css"]     , "settings": { "foreground": "#d7ba7d"  } },

    { "scope": ["meta.tag"]                            , "settings": { "foreground": "#808080"  } },
    { "scope": ["meta.selector"]                       , "settings": { "foreground": "#d7ba7d"  } },
    { "scope": ["meta.structure.dictionary.key.python"], "settings": { "foreground": "#9cdcfe"  } },
    { "scope": ["meta.preprocessor"]                   , "settings": { "foreground": "#ce9178"  } },
    { "scope": ["meta.preprocessor.directive"]         , "settings": { "foreground": "#cda869"  } },
    { "scope": ["meta.preprocessor.string"]            , "settings": { "foreground": "#8f9d6a"  } },
    { "scope": ["meta.preprocessor.numeric"]           , "settings": { "foreground": "#78cf8a"  } },

    { "scope": ["support.assert"]                      , "settings": { "foreground": "#ff6c80", "fontStyle": "italic" } },
    { "scope": ["support.asm",
                "support.likely-unlikely"]             , "settings": { "foreground": "#9a5454", "fontStyle": "italic" } },
    { "scope": ["support.constant.success",
                "support.error-detection.success"]     , "settings": { "foreground": "#55C355", "fontStyle": "italic" } },
    { "scope": ["support.constant.signal",
                "support.constant.failure",
                "support.error-detection.failure"]     , "settings": { "foreground": "#ff4242", "fontStyle": "italic" } },

    { "scope": ["support.function-name.std"]           , "settings": { "fontStyle": "italic" } },

    { "scope": ["support.all-upper"]                   , "settings": { "foreground": "#ce9178"  } },
    { "scope": ["support.first-upper"]                 , "settings": { "foreground": "#42b3c5"  } },

    { "scope": ["storage",
                "storage.type",
                "storage.type.access",
                "storage.modifier.const",
                "keyword.control",
                "keyword.operator.noexcept",
                "keyword.operator.sizeof",
                "constant.language",
                "constant.nullptr",
                "support.something-t",
                "support.type.std",
                "support.type.stdint",
                "support.type.winstd",
                "support.type.winapi"]                 , "settings": { "foreground": "#6199d4"  } },

    { "scope": ["storage.modifier"]                    , "settings": { "foreground": "#869eb7"  } },
    { "scope": ["storage.modifier.virtual.cpp",
                "storage.modifier.override.cpp",
                "storage.modifier.final.cpp"]          , "settings": { "foreground": "#6199d4", "fontStyle": "bold" } },
    { "scope": ["namespace-separator"]                 , "settings": { "foreground": "#fa8d6a", "fontStyle": "bold" } },

    { "scope": ["support.constant.std",
                "support.constant.k-something",
                "comment.docgroup",
                "comment.variable"]                    , "settings": { "foreground": "#51b1ce", "fontStyle": "italic" } },

    { "scope": ["string",
                "string.tag",
                "punctuation.single.quoted.string",
                "punctuation.double.quoted.string"]    , "settings": { "foreground": "#8f9d6a"  } },
    { "scope": ["character.escape"]                    , "settings": { "foreground": "#EDC869"  } },
    { "scope": ["character.format"]                    , "settings": { "foreground": "#AfFd8a"  } },
    { "scope": ["string.regexp"]                       , "settings": { "foreground": "#d16969"  } },

    { "scope": ["support.property-name"]               , "settings": { "foreground": "#cda869"  } },
    { "scope": ["variable.language.this",
                "first-lower-underscore-prefix",
                "first-lower-underscore-suffix"]       , "settings": { "foreground": "#9b859d"  } },
    { "scope": ["support.double-underscore-prefix.cpp"], "settings": { "foreground": "#569cd6"  } },
    { "scope": ["keyword.control.less"]                , "settings": { "foreground": "#d7ba7d"  } },
    { "scope": ["keyword.operator",
                "keyword.operator.cast"]               , "settings": { "foreground": "#fa8d6a"  } },
    { "scope": ["keyword.operator.new",
                "keyword.operator.delete"]             , "settings": { "foreground": "#569cd6"  } },
    { "scope": ["keyword.other.unit"]                  , "settings": { "foreground": "#b5cea8"  } },
    { "scope": ["support.function.git-rebase"]         , "settings": { "foreground": "#d4d4d4"  } },
    { "scope": ["constant.sha.git-rebase"]             , "settings": { "foreground": "#b5cea8"  } },
    { "scope": ["storage.modifier.import.java",
                "storage.modifier.package.java"]       , "settings": { "foreground": "#d4d4d4"  } }
  ]
}
