{
  "name": "reloaded-themes",
  "displayName": "Reloaded Themes",
  "description": "Themes designed for 'Reloaded C/C++' extension",
  "version": "0.2.1",
  "publisher": "reloadedextensions",
  "engines": { "vscode": "^1.9.0" },
  "categories": ["Themes"],
  "repository": {
    "type": "git",
    "url": "https://github.com/kobalicek/reloaded-themes.git"
  },
  "bugs": {
    "url": "https://github.com/kobalicek/reloaded-themes/issues"
  },
  "contributes": {
    "themes": [{
